    "int8",
    "int16",
    "int32",
    "uint8",
    "uint16",
    "uint32",
    "const",
    "constant",
    "util",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/compute-io/intmax/issues"
  },
  "dependencies": {
    "compute-const-max-int16": "^1.0.0",
    "compute-const-max-int32": "^1.0.0",
    "compute-const-max-int8": "^1.0.0",
    "compute-const-max-uint16": "^1.0.0",
    "compute-const-max-uint32": "^1.0.0",
    "compute-const-max-uint8": "^1.0.0",
    "validate.io-string-primitive": "^1.0.0"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "mocha": "2.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x"
  },
  "license": "MIT"
}
